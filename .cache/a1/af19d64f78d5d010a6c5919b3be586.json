ginal":{"line":65,"column":26}},{"generated":{"line":91,"column":37},"source":"components/login-view/login-view.jsx","original":{"line":62,"column":22}},{"name":"onRegister","generated":{"line":92,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":4}},{"name":"onRegister","generated":{"line":92,"column":2},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":4}},{"generated":{"line":92,"column":12},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":14}},{"name":"PropTypes","generated":{"line":92,"column":14},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":16}},{"name":"func","generated":{"line":92,"column":33},"source":"components/login-view/login-view.jsx","original":{"line":66,"column":26}},{"generated":{"line":93,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":62,"column":22}},{"generated":{"line":93,"column":1},"source":"components/login-view/login-view.jsx","original":{"line":62,"column":0}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport PropTypes from 'prop-types';\nimport axios from 'axios';\nimport './login-view.scss';\n\n\n// Use of React hooks with use state, with the consequence that \"use state\" replaces the keyword \"this\" entirely\nexport function LoginView({\n  onLoggedIn\n}) {\n  const [ username, setUsername ] = useState('');\n  const [ password, setPassword ] = useState('');\n\n  const handleSubmit = (e) => {\n\n    // Prevent page refresh\n    e.preventDefault();\n\n    // Sends a request to the server for authentication\n    axios\n      .post(\n        'https://myflixdb21.herokuapp.com/login',\n        {\n          Username: username,\n          Password: password\n        }\n      )\n      .then(\n        (response) => {\n          const data = response.data;\n          // Send 'username' to onLoggedIn()\n          onLoggedIn(data);\n        }\n      )\n      .catch(\n        (err) => {\n          console.log('User not found.')\n        }\n      );\n  };\n\n    /* then call props.onLoggedIn(username) */\n    // props.onLoggedIn(username);\n  /*};*/\n\n  return (\n    <form>\n      <label>\n        Username:\n        <input type=\"text\" value={username} onChange={e => setUsername(e.target.value)} />\n      </label>\n      <label>\n        Password:\n        <input type=\"password\" value={password} onChange={e => setPassword(e.target.value)} />\n      </label>\n      <button type=\"submit\" onClick={handleSubmit}>Submit</button>\n    </form>\n  );\n}\n\n// Using propTypes to make sure the props have been passed properly in terms of format & completeness\nLoginView.propTypes = {\n    username: PropTypes.string,\n    password: PropTypes.string,\n    onLoggedIn: PropTypes.func, \n    onRegister: PropTypes.func\n};\n"},"lineCount":null}},"error":null,"hash":"8f05ae795b2c9346f90b443310f41742","cacheData":{"env":{}}}